{
  "name": "whoswho",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --watch",
    "dev": "concurrently --kill-others --kill-others-on-fail 'npm run dynamo' 'npm run watch:dev'",
    "watch:dev": "nodemon ./src/index.js",
    "dynamo": "java -Djava.library.path=~/dynamodb_local_latest/DynamoDBLocal_lib/ -jar ~/dynamodb_local_latest/DynamoDBLocal.jar",
    "json:server": "json-server --watch ./src/db/db.json",
    "create-db": "node ./src/db/createTable.js",
    "drop-db": "node ./src/db/deleteTable.js",
    "load-db": "node ./src/db/loadDB.js",
    "scan-db": "node ./src/db/scanDB.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.0.0",
    "aws-sdk": "^2.286.2",
    "axios": "^0.18.0",
    "graphql": "^0.13.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "jest": "^23.5.0",
    "jest-dom": "^1.12.0",
    "json-server": "^0.14.0",
    "nodemon": "^1.18.3"
  },
  "jest": {
    "bail": true,
    "verbose": false,
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ]
  }
}
